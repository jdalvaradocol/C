
Algoritmo ciclos
	
	Definir indice, dato Como Entero
	
	Para indice<-0 Hasta 10 Con Paso 1 Hacer
		
		Escribir  "Para Indice = " indice 
		
	Fin Para
	
	indice <- 0
	
	Repetir
		Escribir  "Repetir Indice = " indice
		indice <- indice + 1
	Hasta Que indice > 10
	
	indice <- 0

	Mientras indice <= 10 Hacer
		Escribir  "Mientras Indice = " indice
		indice <- indice + 1
	Fin Mientras
	
FinAlgoritmo
